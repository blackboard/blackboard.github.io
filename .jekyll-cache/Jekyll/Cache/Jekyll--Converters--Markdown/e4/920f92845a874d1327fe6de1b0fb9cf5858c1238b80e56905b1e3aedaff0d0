    Â Â Â Â Â Â Â Â Â Â Â  catch (Exception ex)
    Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Debug.WriteLine(@" ERROR {0}", ex.Message);
    Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  return membership;
    Â Â Â Â Â Â Â  }
</code></pre></div></div>

<p><strong>Update</strong></p>
<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>    Â Â Â Â Â Â Â  public async Task&lt;Membership&gt; UpdateObject(Membership updateMembership)
    Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â  Membership membership = new Membership();
    Â Â Â Â Â Â Â Â Â Â Â  try
    Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  var json = JsonConvert.SerializeObject(updateMembership);
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  var body = new StringContent(json, Encoding.UTF8, "application/json");
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  HttpResponseMessage response = await HttpClientExtensions.PatchAsync(client, Constants.HOSTNAME + Constants.COURSE_PATH + "/externalId:" + Constants.COURSE_ID + "users/externalId:" + Constants.USER_ID, body);
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if (response.IsSuccessStatusCode)
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Debug.WriteLine(@" Membership successfully updated.");
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  var content = await response.Content.ReadAsStringAsync();
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  membership = JsonConvert.DeserializeObject&lt;Membership&gt;(content);
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  catch (Exception ex)
    Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Debug.WriteLine(@" ERROR {0}", ex.Message);
    Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  return (membership);
    Â Â Â Â Â Â Â  }
</code></pre></div></div>

<p><strong>Delete</strong></p>
<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>    Â Â Â Â Â Â Â  public async Task&lt;Membership&gt; DeleteObject()
    Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â  Membership membership = new Membership();
    Â Â Â Â Â Â Â Â Â Â Â  var uri = new Uri(Constants.HOSTNAME + Constants.COURSE_PATH + "/externalId:" + Constants.COURSE_ID + "users/externalId:" + Constants.USER_ID);
    Â Â Â Â Â Â Â Â Â Â Â  try
    Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  var response = await client.DeleteAsync(uri);
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if (response.IsSuccessStatusCode)
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Debug.WriteLine(@" Membership successfully deleted.");
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  var content = await response.Content.ReadAsStringAsync();
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  membership = JsonConvert.DeserializeObject&lt;Membership&gt;(content);
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  catch (Exception ex)
    Â Â Â Â Â Â Â Â Â Â Â  {
    Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Debug.WriteLine(@" ERROR {0}", ex.Message);
    Â Â Â Â Â Â Â Â Â Â Â  }
    Â Â Â Â Â Â Â Â Â Â Â  return (membership);
    Â Â Â Â Â Â Â  }
</code></pre></div></div>

<h2 id="conclusion">Conclusion</h2>

<p>All of the code snippets included in this document at included in a sample
REST Demo C# application available on
<a href="https://github.com/blackboard/BBDN-REST-Demo-CSharp">GitHub</a>.
There is a README.html included that talks more specifically about building and
running the code. Feel free to review the code and run it against a test or
development Learn instance to see how it works.</p>

